<%layout('layouts/boilerplate')-%>
     
    <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-1">
    <%for(list of allListings){%>
        <a href="/listings/<%=list._id%>" class="card-redirect">
        <div class="card col">
        <img src="<%=list.image.url%>" class="card-img-top" alt="listing_image" style="height: 20rem;">
        <div class="card-img-overlay">On Discount</div>
        <div class="card-body">
        <h5 class="card-title"><b><%=list.title%></b></h5>
        <p class="card-text">Price : â‚¹<%=list.price.toLocaleString("en-IN")%> per night.</p>
        </div>
        </div>
        </a>
     <%}%>
    </div>
   